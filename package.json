{
  "name": "website-blocker-extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "move": "cp manifest.json dist/manifest.json && cp -r icons dist/icons",
    "build": "vite build && pnpm move",
    "export": "rm focusmate-extension.zip && zip -r focusmate-extension.zip dist",
    "watch": "vite build --watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.2"
  }
}
